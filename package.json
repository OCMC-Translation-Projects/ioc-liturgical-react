    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-bootstrap": "0.30.7",
    "react-bootstrap-date-picker": "4.0.0",
    "react-bootstrap-table": "3.1.7",
    "react-checkbox-list": "0.0.2",
    "react-dom": "^15.4.2",
    "react-file-download": "^0.3.2",
    "react-fontawesome": "^1.5.0 ",
    "react-google-charts": "^1.5.5",
